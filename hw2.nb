Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"newton", "[", 
    RowBox[{"f_", ",", "x0_", ",", "maxiter_", ",", "thres_"}], "]"}], " ", ":=",
    " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"x", " ", "=", " ", "x0"}], ",", 
       RowBox[{"i", "=", "0"}]}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"g", "[", "x_", "]"}], ":=", 
       RowBox[{"x", " ", "-", " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"f", "[", "x", "]"}], "/", 
          RowBox[{
           RowBox[{"f", "'"}], "[", "x", "]"}]}], ")"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"Abs", "[", 
           RowBox[{"f", "[", "x", "]"}], "]"}], " ", "\[GreaterEqual]", " ", 
          "thres"}], " ", "&&", " ", 
         RowBox[{"i", " ", "\[LessEqual]", " ", "maxiter"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"x", " ", "=", " ", 
